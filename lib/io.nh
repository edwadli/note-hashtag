include types

extern "support.hpp" "nh_support" "print_string" fun Print string -> unit

fun PrintEndline str = (
  //Prints the string followed by a newline to standard out
  Print str
  Print "\n"
)

fun PrintInt n = Print (StringOfInt n)

fun PrintFloat n = Print (StringOfFloat n)

fun PrintBool n = Print (StringOfBool n)
